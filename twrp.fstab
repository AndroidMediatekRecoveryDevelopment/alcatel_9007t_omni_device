/system     ext4  /dev/block/bootdevice/by-name/system
/custpack   ext4  /dev/block/bootdevice/by-name/custpack   
/data       ext4  /dev/block/bootdevice/by-name/userdata    length=-16384
/cache      ext4  /dev/block/bootdevice/by-name/cache
/boot       emmc  /dev/block/bootdevice/by-name/boot
/recovery   emmc  /dev/block/bootdevice/by-name/recovery
/frp        emmc  /dev/block/bootdevice/by-name/config
/misc      	emmc  /dev/block/bootdevice/by-name/misc
/persist 	ext4  /dev/block/bootdevice/by-name/persist  
/modemst1   emmc  /dev/block/bootdevice/by-name/modemst1
/modemst2   emmc  /dev/block/bootdevice/by-name/modemst2
/keystore   emmc  /dev/block/bootdevice/by-name/keystore
/abootbak   emmc  /dev/block/bootdevice/by-name/abootbak

/external_sd  auto  /dev/block/mmcblk1p1  /dev/block/mmcblk1  flags=display="Micro SDcard";storage;wipeingui;removable;andsec
